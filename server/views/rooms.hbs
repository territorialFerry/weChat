<body>
  <div>
    <h1>WeChat</h1>
  </div>

  <div>
    <form method="post" action="http://vokoshyv.com/wechat/users/logout">
      <input type="submit" value="Logout">
    </form>
  </div>

  <div>
    <ul>Rooms</ul>
    {{#each usersPerRoom}}
      <li><a href="http://vokoshyv.com/wechat/rooms/{{@key}}">{{@key}} ({{this}} online)</a></li>
    {{/each}}
  </div>

